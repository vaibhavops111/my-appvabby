node{
    stage('SCM Checkout'){
   
       git 'https://github.com/vaibhavops111/my-appvabby'
    }
    stage('Compile-Package'){
       // Get maven home path
       def  mvnHome = tool name: 'maven-3', type: 'maven'
       sh "${mvnHome}/bin/mvn package"
     }
     stage('Email Notification'){
         mail bcc: '', body: '''Hi welcome to jenkins  email alerts
        Thanks
         Vabby''', cc: '', from: '', replyTo: '', subject: 'jenkins job', to: 'vaibhav.08patil@gmail.com'
     }    
     stage('Slack Notification')
      slackSend baseUrl: 'https://hooks.slack.com/services/', 
      channel: '#jenkins-pipeline-deom', 
      color: 'good', 
      message: 'WElcome to Jenkins, Slack!', 
      teamDomain: 'https://hooks.slack.com/services/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', 
      tokenCredentialId: 'slack-demo', username: 'vabby111'
    } 
}     
