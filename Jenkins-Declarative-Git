pipeline{
    agent any
    
    environment{
         PATH = "/opt/mvn3/bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git 'https://github.com/vaibhavops111/my-appvabby'
            }
        }
        stage("Maven Build"){
             steps{
                sh "mvn clean package"
             
             }
             
        }
    }
   
}
